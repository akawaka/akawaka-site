{
    "name": "mono/core-bundle",
    "description": "Core infrastructure bundle for Symfony",
    "type": "mono-symfony-bundle",
    "license": "EUPLv1-2",
    "authors": [
        {
            "name": "Alexandre Balmes",
            "email": "oss@alexandre.balmes.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mono\\Bundle\\CoreBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mono\\Bundle\\CoreBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "league/flysystem-bundle": "^2.0",
        "ramsey/uuid": "^4.1",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/orm-pack": "^2.1",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/security-bundle": "^5.2",
        "symfony/validator": "^5.2",
        "thecodingmachine/safe": "^1.3",
        "webmozart/assert": "^1.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.13",
        "league/flysystem-memory": "^2.0",
        "phpspec/phpspec": "^7.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "vimeo/psalm": "^3.11",
        "roave/security-advisories": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
