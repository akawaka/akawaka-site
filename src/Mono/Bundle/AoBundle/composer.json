{
    "name": "mono/ao-bundle",
    "description": "Content bundle for Symfony",
    "type": "mono-bundle",
    "license": "EUPLv1-2",
    "authors": [
        {
            "name": "Alexandre Balmes",
            "email": "oss@alexandre.balmes.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mono\\Bundle\\AoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mono\\Tests\\Bundle\\AoBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "mono/core-bundle": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.13",
        "league/flysystem-memory": "^2.0",
        "phpspec/phpspec": "^7.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "vimeo/psalm": "^3.11",
        "roave/security-advisories": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
