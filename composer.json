{
    "name": "mono/ao",
    "description": "ao",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.9",
        "league/flysystem-bundle": "^2.0",
        "psr/log": "^1.1",
        "qossmic/rich-model-forms-bundle": "^0.1.0",
        "ramsey/uuid": "^4.1",
        "sylius/theme-bundle": "^2.2",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/notifier": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/workflow": "5.2.*",
        "symfony/yaml": "5.2.*",
        "thecodingmachine/safe": "^1.3",
        "twig/extra-bundle": "^3.3",
        "twig/intl-extra": "^3.3",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Mono\\Bundle\\UIBundle\\": "src/Mono/Bundle/UIBundle/src/",
            "Mono\\Bundle\\CoreBundle\\": "src/Mono/Bundle/CoreBundle/src/",
            "Mono\\Component\\AdminSecurity\\": "src/Mono/Component/AdminSecurity/src/",
            "Mono\\Component\\Article\\": "src/Mono/Component/Article/src/",
            "Mono\\Component\\Space\\": "src/Mono/Component/Space/src/",
            "Mono\\Component\\Core\\": "src/Mono/Component/Core/src/",
            "Mono\\Component\\Page\\": "src/Mono/Component/Page/src/",
            "Mono\\Primitive\\EmailAddress\\": "src/Mono/Primitive/EmailAddress/src/"
        },
        "exclude-from-classmap": [
            "src/Mono/*/*/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Mono\\Bundle\\AoBundle\\Tests\\": "src/Mono/Bundle/AoBundle/tests/"
        }
    },
    "replace": {
        "mono/admin-security": "^1.0",
        "mono/article": "^1.0",
        "mono/ao-bundle": "^1.0",
        "mono/channel": "^1.0",
        "mono/core": "^1.0",
        "mono/core-bundle": "^1.0",
        "mono/email-address-primitive": "^1.0",
        "mono/page": "^1.0",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.2.*"
        },
        "merge-plugin": {
            "include": [
                "src/Mono/*/*/composer.json"
            ],
            "recurse": true,
            "replace": true,
            "ignore-duplicates": true,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "friends-of-behat/symfony-extension": "^2.2",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpspec/phpspec": "^7.0",
        "phpstan/phpstan": "^0.12.82",
        "phpstan/phpstan-symfony": "^0.12.21",
        "psalm/plugin-symfony": "^2.2",
        "rector/rector": "^0.10.3",
        "rector/rector-symfony": "^0.10.0",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/debug-bundle": "^5.2",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/var-dumper": "^5.2",
        "symfony/web-profiler-bundle": "^5.2",
        "symplify/config-transformer": "^9.2",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "vimeo/psalm": "^4.6",
        "webmozart/assert": "^1.10"
    }
}
